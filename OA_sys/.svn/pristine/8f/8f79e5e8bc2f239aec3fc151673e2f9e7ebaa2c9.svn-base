package clt.com.cn.dao.impl;

import java.util.List;

import clt.com.cn.base.BaseDao;
import clt.com.cn.dao.IDeptDao;
import clt.com.cn.dao.IEducationlevelDao;
import clt.com.cn.model.entity.*;


public class EducationlevelDao extends BaseDao implements IEducationlevelDao {
	
	public void addEducationlevel(Educationlevel educationlevel) {
		super.addObject(educationlevel);
	}
	public void delEducationlevelById(int id) {
		super.deleteObjectById(Educationlevel.class, id);
	}
	public Educationlevel getEducationlevelById(int id) {
		return (Educationlevel) super.getObjectById(Educationlevel.class, id);
	}
	public void updateEducationlevel(Educationlevel d) {
		super.updateObject(d);
	}
	public List<Educationlevel> getAllEducationlevel() {
		// TODO Auto-generated method stub
		return super.getAllObject(Educationlevel.class);
	}

}
